; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="NeckApp"
run/main_scene="res://Scenes/MainMenu/MainMenu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="NeckApp"
config/project_settings_override="user://config.godot"
config/features=PackedStringArray("4.0", "GL Compatibility")
boot_splash/show_image=false
config/icon="res://icon.svg"

[audio]

driver/enable_input=true

[autoload]

GlobalSettings="*res://Autoload/GlobalSettings.gd"
GlobalVars="*res://Autoload/GlobalVars.gd"

[debug]

settings/stdout/verbose_stdout=true
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[input_devices]

pointing/ios/touch_delay=0.0
pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="gl_compatibility"
